{
  "presets":[
    "react",
    "es2015",
    ["env",{
      "targets": {
        "browsers":"last 2 versions"
      },
      "loose":true,
      "modules":false
    }
    ]
  ],
  "plugins": [
    "babel-plugin-transform-class-properties"
  ],
  "env":{
    "test": {
      "plugins": [
        "react-hot-loader/babel",
        "transform-es2015-modules-commonjs"
      ]
        }
  }
}
